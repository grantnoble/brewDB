CREATE TABLE styles
(
styles_id mediumint unsigned NOT NULL auto_increment,
styles_name varchar(255) NOT NULL,
styles_category varchar(255) NOT NULL,
styles_version tinyint unsigned NOT NULL default 1,
styles_category_number varchar(255) NOT NULL,
styles_style_letter varchar(255) NOT NULL,
styles_style_guide varchar(255) NOT NULL,
styles_type enum('Lager', 'Ale', 'Mead', 'Wheat', 'Mixed', 'Cider'),
styles_og_min float,
styles_og_max float,
styles_fg_min float,
styles_fg_max float,
styles_ibu_min float,
styles_ibu_max float,
styles_color_min float,
styles_color_max float,
styles_carb_min float,
styles_carb_max float,
styles_abv_min float,
styles_abv_max float,
styles_notes varchar(255),
styles_profile varchar(255),
styles_ingredients varchar(255),
styles_examples varchar(255),
PRIMARY KEY (styles_id),
INDEX (styles_name),
INDEX (styles_category),
INDEX (styles_category_number),
INDEX (styles_style_letter),
INDEX (styles_type)
) AUTO_INCREMENT=50001;
